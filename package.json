{
  "name": "avantio",
  "version": "1.0.0",
  "description": "Web scraper con Playwright",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node server.ts",
    "dev:watch": "ts-node-dev --respawn server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "scraper",
    "playwright",
    "web-scraping"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.18.3",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "playwright": "^1.56.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/express": "^4.17.21",
    "@types/node": "^24.8.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
